{
    "version": 2,
    "builds": [
      {
        "src": "./Server.js",
        "use": "@vercel/node"
      },
      {
        "src": "./vite.config.js",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "./dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "./Server.js"
      }
    ]
  }
  